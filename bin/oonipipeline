#!/usr/bin/env python
import os
import sys

sys.path[:] = map(os.path.abspath, sys.path)
sys.path.insert(0, os.path.dirname(os.path.dirname(os.path.abspath(__file__))))

from ooni.pipeline import run, usage
if __name__ == "__main__":
    if len(sys.argv) < 2:
        usage()
    task_name = sys.argv[1]
    run(task_name)
